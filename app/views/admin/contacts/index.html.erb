<div class="container mb-5">
  <h5>お問合せ一覧</h5>
  <div class="mt-3">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>名前</th>
          <th>ニックネーム</th>
          <th>テーマ</th>
        </tr>
      </thead>
      <tbody>
        <% @contacts.each do |contact| %>
        <tr onclick='window.location="<%= '/admin/contacts/' + contact.id.to_s %>"' role="link">
          <td><%= contact.name %></td>
          <td><%= contact.saunner.nickname %></td>
          <td><%= contact.title %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <div class="paginate"><%= paginate @contacts %></div>
  </div>
</div>